{
  "name": "react-pc-building-site",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "server": "node ./dist-tsc/js/server/server.js",
    "dev": "concurrently --kill-others --kill-others-on-fail -n \":\" -c green,cyan,magenta \"npm:dev-*\"",
    "dev-web": "vite",
    "dev-server": "sleep 3 && nodemon -w \"./dist-tsc/js/\" ./dist-tsc/js/server/server.js",
    "dev-server-ts": "chokidar --initial \"./server/\" \"./types/\" -c \"npm run build-server\"",
    "build": "concurrently -c cyan,magenta \"npm:build-*\"",
    "build-web": "tsc && vite build",
    "build-server": "tsc --build ./tsconfig.server.json --verbose --pretty",
    "preview": "vite preview",
    "init-db": "node ./dist-tsc/js/server/docker-init-db.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/jwt-decode": "^3.1.0",
    "bcrypt": "^5.1.0",
    "big.js": "^6.2.1",
    "bootstrap": "^5.2.0",
    "connect-mongo": "^4.6.0",
    "consolidate": "^0.16.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^4.10.0",
    "mongoose": "^6.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.4.1",
    "redis": "^4.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@types/big.js": "^6.1.6",
    "@types/bootstrap": "^5.2.4",
    "@types/consolidate": "^0.14.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.17",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/react": "^18.0.17",
    "@types/react-bootstrap": "^0.32.30",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-react": "^2.0.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "sass": "^1.54.4",
    "terser": "^5.15.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vitest": "^0.24.0"
  }
}
