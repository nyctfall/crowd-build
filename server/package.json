{
  "name": "~server",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "node ../dist-tsc/server.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "conc --kill-others-on-fail --prefix-colors cyan,magenta \"npm run watch-*\"",
    "watch-srv": "nodemon --watch \"../dist-tsc\" ../dist-tsc/server.js",
    "watch-tsc": "npm run build -- --watch --preserveWatchOutput",
    "build": "tsc --build --verbose --pretty",
    "init-db": "node ../dist-tsc/docker-init-db.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "connect-mongo": "^4.6.0",
    "consolidate": "^0.16.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.12.0",
    "mongoose": "^6.7.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redis": "^4.5.0",
    "~types": "link:../types"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/consolidate": "^0.14.1",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "prettier-eslint-cli": "^7.1.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.2"
  }
}
